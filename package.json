    "url": "https://github.com/MartinaRia/sass-teambit/issues"
  },
  "homepage": "https://github.com/MartinaRia/sass-teambit#readme",
  "devDependencies": {
    "laravel-mix": "^5.0.4",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "cross-env": "^7.0.2"
  }
